<div class="row">
  <div class="text-center">
    <h2>会員一覧</h2>
    <div class="row">
      <div class="enduser-index">
        <table>
          <thead>
            <tr>
              <th>会員ID</th>
              <th>氏名</th>
              <th>メールアドレス</th>
              <th>ステータス</th>
            </tr>
          </thead>
          <%= @endusers.each do |enduser| %>
            <tbody>
              <tr>
                <td><%= enduser.id %></td>
                <td><%= link_to enduser.name, hostuser_path %></td>
                <td><%= enduser.email %></td>
                <td><%= enduser.status %></td>
              </tr>
            </tbody>
          <% end %>
        </table>
      </div>
    </div>
  </div>
</div>
